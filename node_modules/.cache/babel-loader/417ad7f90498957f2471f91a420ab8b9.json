{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport router from '@/router';\nimport axios from 'axios';\nconst listData = [];\nlet url = 'http://localhost:8082/checkBox';\nlet url2 = 'http://localhost:8082/checkBoxNum';\naxios.post(url, {}).then(res => {\n  for (let i = 0; i < res.data.length; i++) {\n    console.log();\n    listData.push({\n      title: res.data[i].article_title,\n      description: res.data[i].date,\n      content: res.data[i].article_introduction,\n      topic: res.data[i].topic\n    });\n  }\n\n  console.log(res.data);\n});\nexport default {\n  data() {\n    return {\n      num1: '',\n      num2: '',\n      listData,\n      pagination: {\n        onChange: page => {\n          console.log(page);\n        },\n        pageSize: 3\n      },\n      actions: [{\n        type: 'share-alt',\n        text: '156'\n      }, {\n        type: 'like-o',\n        text: '156'\n      }, {\n        type: 'message',\n        text: '2'\n      }]\n    };\n  },\n\n  methods: {\n    jumpToArticle() {\n      router.push({\n        path: '/article/' + this.articlePath\n      });\n    },\n\n    chakan() {\n      axios.post(url2, {}).then(res1 => {\n        console.log(res1.data);\n        this.num1 = res1.data.finish, this.num2 = res1.data.unFinish;\n      });\n    }\n\n  },\n  components: {\n    router\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqCA;AACA;AACA;AACA;AACA;AACAA;EACA;IACAC;IACAC;MACAC,gCADA;MAEAC,6BAFA;MAGAC,yCAHA;MAIAC;IAJA;EAMA;;EACAL;AACA,CAXA;AAcA;EACAM;IACA;MACAC,QADA;MAEAC,QAFA;MAGAP,QAHA;MAIAQ;QACAC;UACAV;QACA,CAHA;QAIAW;MAJA,CAJA;MAUAC,UACA;QAAAC;QAAAC;MAAA,CADA,EAEA;QAAAD;QAAAC;MAAA,CAFA,EAGA;QAAAD;QAAAC;MAAA,CAHA;IAVA;EAgBA,CAlBA;;EAmBAC;IACAC;MACAC;QAAAC;MAAA;IACA,CAHA;;IAIAC;MACApB;QACAC;QAEA,8BACA,8BADA;MAGA,CANA;IAOA;;EAZA,CAnBA;EAkCAoB;IAAAH;EAAA;AAlCA","names":["axios","console","listData","title","description","content","topic","data","num1","num2","pagination","onChange","pageSize","actions","type","text","methods","jumpToArticle","router","path","chakan","components"],"sourceRoot":"src/components","sources":["check.vue"],"sourcesContent":["<template>\r\n  <el-container style=\"width: 1000px;margin: 10px 17%;\">\r\n    <el-header style=\"height: 90px;background-color: white;\">\r\n      <h1 style=\"font-size: 25px;font-weight: 700;margin: 0 400px;\">审核箱</h1>\r\n      <el-button type=\"primary\" plain style=\"margin: -50px 100px;\">主要按钮</el-button>\r\n      <div style=\"margin: -20px 290px;\">\r\n        <el-tag type=\"success\" style=\"margin: 20px 20px\">已审核文章：{{ num1 }}</el-tag>\r\n        <el-tag type=\"info\" style=\"margin: 20px 20px\">未审核文章：{{ num2 }}</el-tag>\r\n        \r\n\r\n      </div>\r\n      \r\n    </el-header>\r\n\r\n    <el-main style=\"height: 590px;\">\r\n      <a-list item-layout=\"vertical\" size=\"large\" :pagination=\"pagination\" :data-source=\"listData\">\r\n        <div slot=\"footer\"></div>\r\n        <a-list-item slot=\"renderItem\" key=\"item.title\" slot-scope=\"item, index\">\r\n\r\n\r\n          <a-list-item-meta :description=\"item.description\">\r\n            <a slot=\"title\" @click=\"jumpToArticle\">{{ item.title }}</a>\r\n\r\n          </a-list-item-meta>\r\n          {{ item.content }}\r\n          <div style=\"margin: 0px 700px;width: 200px;\">\r\n            <el-button type=\"success\" size=\"small\" plain>通过审核</el-button>\r\n            <el-button type=\"danger\" size=\"small\" plain>驳回文章</el-button>\r\n          </div>\r\n\r\n        </a-list-item>\r\n\r\n      </a-list>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n<script>\r\nimport router from '@/router';\r\nimport axios from 'axios';\r\nconst listData = [];\r\nlet url = 'http://localhost:8082/checkBox';\r\nlet url2 = 'http://localhost:8082/checkBoxNum';\r\naxios.post(url, {}).then(res => {\r\n  for (let i = 0; i < res.data.length; i++) {\r\n    console.log()\r\n    listData.push({\r\n      title: res.data[i].article_title,\r\n      description: res.data[i].date,\r\n      content: res.data[i].article_introduction,\r\n      topic: res.data[i].topic\r\n    });\r\n  }\r\n  console.log(res.data)\r\n})\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      num1: '',\r\n      num2: '',\r\n      listData,\r\n      pagination: {\r\n        onChange: page => {\r\n          console.log(page);\r\n        },\r\n        pageSize: 3,\r\n      },\r\n      actions: [\r\n        { type: 'share-alt', text: '156' },\r\n        { type: 'like-o', text: '156' },\r\n        { type: 'message', text: '2' },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    jumpToArticle() {\r\n      router.push({ path: '/article/' + this.articlePath });\r\n    },\r\n    chakan() {\r\n      axios.post(url2, {}).then(res1 => {\r\n        console.log(res1.data)\r\n\r\n        this.num1 = res1.data.finish,\r\n          this.num2 = res1.data.unFinish\r\n\r\n      })\r\n    }\r\n  },\r\n\r\n  components: { router }\r\n};\r\n\r\n</script>"]},"metadata":{},"sourceType":"module"}