{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport '../assets/style/common.scss';\nexport default {\n  name: 'HelloWorld',\n\n  data() {\n    return {\n      num1: 0,\n      num2: 1,\n      list1: [1, 2, 3, 4, 5, 6],\n      val1: 23333,\n      url: 'https://www.baidu.com',\n      pickerOptions: {\n        disabledDate(time) {\n          return time.getTime() > Date.now();\n        },\n\n        shortcuts: [{\n          text: '今天',\n\n          onClick(picker) {\n            picker.$emit('pick', new Date());\n          }\n\n        }, {\n          text: '昨天',\n\n          onClick(picker) {\n            const date = new Date();\n            date.setTime(date.getTime() - 3600 * 1000 * 24);\n            picker.$emit('pick', date);\n          }\n\n        }, {\n          text: '一周前',\n\n          onClick(picker) {\n            const date = new Date();\n            date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\n            picker.$emit('pick', date);\n          }\n\n        }]\n      },\n      activeIndex2: '1'\n    };\n  },\n\n  props: {\n    msg: Number\n  },\n  computed: {\n    num() {\n      var numSum = this.num1 + this.num2 + this.val1;\n      return numSum;\n    }\n\n  },\n\n  created() {},\n\n  mounted() {\n    this.num1 += 10;\n  },\n\n  methods: {\n    addNum() {\n      this.num2++;\n    },\n\n    addNum2() {\n      this.$emit('addNum');\n    },\n\n    handleSelect(key, keyPath) {\n      console.log(key, keyPath);\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyCA;AACA;EACAA,kBADA;;EAEAC;IACA;MACAC,OADA;MAEAC,OAFA;MAGAC,yBAHA;MAIAC,WAJA;MAKAC,4BALA;MAMAC;QACAC;UACA;QACA,CAHA;;QAIAC;UACAC,UADA;;UAEAC;YACAC;UACA;;QAJA,GAKA;UACAF,UADA;;UAEAC;YACA;YACAE;YACAD;UACA;;QANA,CALA,EAYA;UACAF,WADA;;UAEAC;YACA;YACAE;YACAD;UACA;;QANA,CAZA;MAJA,CANA;MA+BAE;IA/BA;EAiCA,CApCA;;EAqCAC;IACAC;EAD