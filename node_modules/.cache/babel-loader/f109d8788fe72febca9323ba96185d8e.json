xport default {\n  name: 'HelloWorld',\n  data() {\n    return {\n      num1: 0,\n      num2: 1,\n      list1: [1, 2, 3, 4, 5, 6],\n      val1: 23333,\n      url: 'https://www.baidu.com',\n      pickerOptions: {\n        disabledDate(time) {\n          return time.getTime() > Date.now();\n        },\n        shortcuts: [{\n          text: '今天',\n          onClick(picker) {\n            picker.$emit('pick', new Date());\n          }\n        }, {\n          text: '昨天',\n          onClick(picker) {\n            const date = new Date();\n            date.setTime(date.getTime() - 3600 * 1000 * 24);\n            picker.$emit('pick', date);\n          }\n        }, {\n          text: '一周前',\n          onClick(picker) {\n            const date = new Date();\n            date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\n            picker.$emit('pick', date);\n          }\n        }]\n      },\n      activeIndex2: '1'\n    }\n  },\n  props: {\n    msg: Number\n  },\n  computed: {\n    num() {\n      var numSum = this.num1 + this.num2 + this.val1\n      return numSum\n    }\n  },\n  created() {},\n  mounted() {\n    this.num1 += 10\n  },\n  methods: {\n    addNum(){\n      this.num2++\n    },\n    addNum2() {\n      this.$emit('addNum')\n    },\n    handleSelect(key, keyPath) {\n      console.log(key, keyPath);\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}