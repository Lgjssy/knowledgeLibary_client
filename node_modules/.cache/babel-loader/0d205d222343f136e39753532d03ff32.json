.then(res=>{\r\n      router.push({ name: 'PersonalCenter',params:{id:res.data.user_id,username:res.data.user_name,phone_number:res.data.phone_number,about:res.data.about,Fans_number:res.data.followerNumber,articleNumber:res.data.articleNumber,isVisual:isV} });\r\n    })\r\n      \r\n      //alert(\"114514\")\r\n     \r\n    },\r\n  }\r\n})\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}