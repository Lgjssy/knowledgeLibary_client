{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  name: \"login\",\n\n  data() {\n    return {\n      radio: 3,\n      user: {\n        username: \"\",\n        job: \"\",\n        password: \"\",\n        peoplename: \"\"\n      }\n    };\n  },\n\n  created() {// console.log($);\n    // console.log(\"1111\");\n  },\n\n  methods: {\n    doRegister() {\n      if (!this.user.username) {\n        this.$message.error(\"请输入用户名！\");\n        return;\n      } else if (!this.user.peoplename) {\n        this.$message.error(\"请输入姓名！\");\n        return;\n      } else if (!this.user.job) {\n        this.$message.error(\"请输入职位\");\n        return;\n      } else if (this.user.job != null) {\n        if (!this.user.password) {\n          this.$message.error(\"请输入密码！\");\n          return;\n        } else {\n          //this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\n          let url = 'http://localhost:8081/register';\n          axios.post(url, {\n            User_Id: this.user.username,\n            Position: this.user.job,\n            User_Password: this.user.password,\n            User_Name: this.user.peoplename\n          }).then(res => {\n            // console.log(\"输出response.data\", res.data);\n            // console.log(\"输出response.data.status\", res.data.status);\n            if (res.data == 0) {\n              alert(\"注册成功！！\");\n              this.$router.push({\n                path: \"/\"\n              });\n            } else {\n              alert(\"您输入的用户id已存在！\");\n            }\n          });\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmCA;AACA;EACAA,aADA;;EAEAC;IACA;MACAC,QADA;MAEAC;QACAC,YADA;QAEAC,OAFA;QAGAC,YAHA;QAIAC;MAJA;IAFA;EASA,CAZA;;EAaAC,WACA;IACA;EACA,CAhBA;;EAiBAC;IACAC;MACA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;MACA,CAHA,MAGA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;UACA;UACAC,MACAC,IADA,CACAC,GADA,EACA;YACAC,2BADA;YAEAC,uBAFA;YAGAC,iCAHA;YAIAC;UAJA,CADA,EAOAC,IAPA,CAOAC;YACA;YACA;YACA;cACAC;cACA;gBAAAC;cAAA;YACA,CAHA,MAGA;cACAD;YACA;UACA,CAhBA;QAiBA;MACA;IACA;;EArCA;AAjBA","names":["name","data","radio","user","username","job","password","peoplename","created","methods","doRegister","axios","post","url","User_Id","Position","User_Password","User_Name","then","res","alert","path"],"sourceRoot":"src/components","sources":["Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"login clearfix\">\r\n    <div class=\"login-wrap\">\r\n      <el-row type=\"flex\" justify=\"center\">\r\n        <el-form ref=\"loginForm\" :model=\"user\" status-icon label-width=\"80px\">\r\n          <h3>注册</h3>\r\n          <hr>\r\n          <el-form-item prop=\"username\" label=\"用户名\">\r\n            <el-input v-model=\"user.username\" placeholder=\"请输入用户名\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"peoplename\" label=\"姓名\">\r\n            <el-input v-model=\"user.peoplename\" placeholder=\"请输入姓名\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\" label=\"设置密码\">\r\n            <el-input v-model=\"user.password\" show-password placeholder=\"请输入密码\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"job\" label=\"职位\">\r\n           <el-radio-group v-model=\"radio\">\r\n    <el-radio :label=\"3\">备选项</el-radio>\r\n    <el-radio :label=\"6\">备选项</el-radio>\r\n    <el-radio :label=\"9\">备选项</el-radio>\r\n  </el-radio-group>\r\n          </el-form-item>\r\n          <el-form-item>\r\n           \r\n            <el-button type=\"primary\" icon @click=\"doRegister()\" style=\"position:relative;left:50px\">注册账号</el-button>\r\n           \r\n          </el-form-item>\r\n        </el-form>\r\n      </el-row>\r\n    </div>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"login\",\r\n  data() {\r\n    return {\r\n      radio: 3,\r\n      user: {\r\n        username: \"\",\r\n        job: \"\",\r\n        password: \"\",\r\n        peoplename: \"\"\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    // console.log($);\r\n    // console.log(\"1111\");\r\n  },\r\n  methods: {\r\n    doRegister() {\r\n      if (!this.user.username) {\r\n        this.$message.error(\"请输入用户名！\");\r\n        return;\r\n      } else if (!this.user.peoplename) {\r\n        this.$message.error(\"请输入姓名！\");\r\n        return;\r\n      } else if (!this.user.job) {\r\n        this.$message.error(\"请输入职位\");\r\n        return;\r\n      } else if (this.user.job != null) {\r\n          if (!this.user.password) {\r\n            this.$message.error(\"请输入密码！\");\r\n          return;\r\n        } else {\r\n           //this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\r\n           let url = 'http://localhost:8081/register';\r\n          axios\r\n            .post(url, {\r\n              User_Id: this.user.username,\r\n              Position: this.user.job,\r\n              User_Password: this.user.password,\r\n              User_Name: this.user.peoplename\r\n            })\r\n            .then(res => {\r\n               // console.log(\"输出response.data\", res.data);\r\n               // console.log(\"输出response.data.status\", res.data.status);\r\n              if (res.data==0) {\r\n                alert(\"注册成功！！\");\r\n                this.$router.push({ path: \"/\" });\r\n              } else {\r\n                alert(\"您输入的用户id已存在！\");\r\n              }\r\n            });\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n \r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.login {\r\n  position:absolute;\r\n  top:0px;\r\n  left: 0px;\r\n  width: 100%;\r\n  height: 960px;\r\n  z-index: 9999;\r\n  background: url(\"../images/image1.png\") no-repeat;\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n.login-wrap {\r\n  background: white;\r\n  background-size: cover;\r\n  opacity: 0.8;\r\n  width: 600px;\r\n  height: 500px;\r\n  margin: 215px auto;\r\n  overflow: hidden;\r\n  padding-top: 10px;\r\n  line-height: 20px;\r\n}\r\n \r\nh3 {\r\n  color: #0babeab8;\r\n  font-size: 24px;\r\n}\r\nhr {\r\n  background-color: #444;\r\n  margin: 20px auto;\r\n}\r\n \r\n.el-button {\r\n  width: 80%;\r\n  margin-left: -50px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}