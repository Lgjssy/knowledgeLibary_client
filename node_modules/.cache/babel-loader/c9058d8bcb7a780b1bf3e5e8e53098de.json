 from 'axios';\r\nconst listData = [];\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      article_name:this.$route.params.id,\r\n      listData,\r\n      pagination: {\r\n        onChange: page => {\r\n          console.log(page);\r\n        },\r\n        pageSize: 3,\r\n      },\r\n      actions: [\r\n        { type: 'share-alt', text: '156' },\r\n        { type: 'like-o', text: '156' },\r\n        { type: 'message', text: '2' },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n        jumpToArticle(ins) {\r\n            router.push({ path: '/article/'+ins});\r\n        },\r\n        \r\n    },\r\n    created:function(){\r\n      let url2 = 'http://localhost:8082/aboutArticle';\r\n          let url = 'http://localhost:8082/searchArticleByName';\r\n          axios.post(url,{key_word:this.article_name}).then(res=>{\r\n            listData.splice(0, listData.length)\r\n            console.log(res.data)\r\n            for (let i = 0; i < res.data.length; i++) {\r\n          axios.post(url2, { article_id: res.data[i].article_id }).then(res1 => {\r\n            console.log()\r\n            listData.push({\r\n              avatar:require('/src/images/'+res.data[i].article_cover),\r\n              type: 'star-o', text: res1.data.shares,\r\n              type2: 'like-o', text2: res1.data.likes,\r\n              type3: 'message', text3: res1.data.comment,\r\n              title: res.data[i].article_title,\r\n             \r\n              description: res.data[i].date,\r\n              content: res.data[i].article_introduction,\r\n              topic: res.data[i].topic\r\n            });\r\n\r\n          })\r\n\r\n\r\n        }\r\n          })\r\n    },\r\n    components: { router }\r\n};\r\n</script>\r\n<style></style>"]},"metadata":{},"sourceType":"module"}