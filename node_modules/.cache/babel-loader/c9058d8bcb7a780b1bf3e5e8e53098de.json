{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport router from '@/router';\nimport axios from 'axios';\nconst listData = [];\nexport default {\n  data() {\n    return {\n      article_name: this.$route.params.id,\n      listData,\n      pagination: {\n        onChange: page => {\n          console.log(page);\n        },\n        pageSize: 3\n      },\n      actions: [{\n        type: 'share-alt',\n        text: '156'\n      }, {\n        type: 'like-o',\n        text: '156'\n      }, {\n        type: 'message',\n        text: '2'\n      }]\n    };\n  },\n\n  methods: {\n    jumpToArticle(ins) {\n      router.push({\n        path: '/article/' + ins\n      });\n    }\n\n  },\n  created: function () {\n    let url2 = 'http://localhost:8082/aboutArticle';\n    let url = 'http://localhost:8082/searchArticleByName';\n    axios.post(url, {\n      key_word: this.article_name\n    }).then(res => {\n      listData.splice(0, listData.length);\n      console.log(res.data);\n\n      for (let i = 0; i < res.data.length; i++) {\n        axios.post(url2, {\n          article_id: res.data[i].article_id\n        }).then(res1 => {\n          console.log();\n          listData.push({\n            avatar: require('/src/images/' + res.data[i].article_cover),\n            type: 'star-o',\n            text: res1.data.shares,\n            type2: 'like-o',\n            text2: res1.data.likes,\n            type3: 'message',\n            text3: res1.data.comment,\n            title: res.data[i].article_title,\n            description: res.data[i].date,\n            content: res.data[i].article_introduction,\n            topic: res.data[i].topic\n          });\n        });\n      }\n    });\n  },\n  components: {\n    router\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgCA;AACA;AACA;AAGA;EACAA;IACA;MACAC,mCADA;MAEAC,QAFA;MAGAC;QACAC;UACAC;QACA,CAHA;QAIAC;MAJA,CAHA;MAS