font-size:40px;color:#f4725c;padding:10px;margin: -5px;\"><!--标题-->\n          知识库系统\n        </el-col>\n        <el-col :span=\"7\"><!--三项菜单-->\n          <el-menu\n            :default-active=\"$route.path\"\n            class=\"el-menu-demo\"\n            mode=\"horizontal\"\n            router\n            @select=\"handleSelect\"\n            background-color=\"white\"\n            text-color=\"black\"\n            active-text-color=\"#f4725c\">\n            <el-menu-item style=\"font-size: 20px;\" index=\"/home\">\n              首页\n            </el-menu-item>\n            <el-menu-item style=\"font-size: 20px;\" index=\"/myFollow\">\n              我的关注\n            </el-menu-item>    \n            <el-menu-item  style=\"font-size: 20px;\" index=\"/myMessage\">\n              消息\n            </el-menu-item>\n            <el-menu-item  style=\"font-size: 20px;\" index=\"/PersonalCenter\">\n              个人中心\n            </el-menu-item>\n          \n          </el-menu>\n        </el-col>\n        <el-col :span=\"5\"><!--搜索框-->\n          <el-input\n            placeholder=\"搜索\"\n            v-model=\"input_search\"\n                        \n            style=\"margin:5px\"\n            size=\"large\">\n              <i slot=\"prefix\" class=\"el-icon-search\" @click=\"jumpToSearch()\" style=\"font-size:20px;margin:50% 10%\"></i>\n          </el-input>\n          <!--prefix-icon=\"el-icon-search\"-->\n        </el-col>\n        <el-col :span=\"1\"><!--占位的-->\n          <div style=\"color:white\" >1</div>\n        </el-col>\n        <el-col :span=\"1\" ><!--头像-->\n          <el-dropdown @command=\"avatarMenu\">\n            <span class=\"el-dropdown-link\">\n              <el-avatar :size=\"38\" :fit=\"fit\" :src=\"avatar\" /> \n            </span>\n          </el-dropdown>\n        </el-col>\n        <el-col :span=\"2\" ><!--头像-->\n          <router-link to=\"/\" style=\"margin: -5px -5px;\" >退出</router-link>\n        </el-col>\n        <el-col :span=\"3\"><!--写文章按钮-->\n          <el-button type=\"warning\"  color=\"#f4725c\" icon=\"el-icon-edit\" @click=\"jumpToWriter()\">写文章 \n          </el-button>         \n        </el-col>\n      </el-row>\n      \n    \n      \n  </el-header>\n  <router-view/>\n  </el-container>\n  \n</template>\n\n<style>\n\n</style>\n<script>\nimport avatars from\"./assets/avatars.png\";//引入本地的一个头像测试用例\nimport router from \"./router/index.js\"\nexport default({\n\n  data() {\n    return {\n    avatar:avatars,//导入用户中心的头像\n    input_search:''//搜索框里的字符串\n    }\n  },\n  methods: {\n    jumpToWriter: function()//“写文章”按钮的函数\n    {\n      router.push({ path: '/writer' });\n      //alert(\"114514\")\n    },\n    avatarMenu(command)//头像点击的函数\n    {\n      if(command == \"personalCenter\")\n        router.push({ path: '/personalCenter' });\n      //alert(\"114514\")\n    },\n    test()//测试事件是否能正常触发的函数\n    {\n      alert(\"114514\")\n    },\n    jumpToSearch()\n    {\n      router.push({ path: '/search' });\n    },\n    jumpToLogin()\n    {\n      router.push({ path: '/Login' });\n    }\n  }\n})\n</script>\n"]},"metadata":{},"sourceType":"module"}