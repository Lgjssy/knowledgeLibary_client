{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  name: \"login\",\n\n  data() {\n    return {\n      user: {\n        username: \"\",\n        job: \"\",\n        password: \"\",\n        peoplename: \"\"\n      },\n      imageUrl: ''\n    };\n  },\n\n  created() {// console.log($);\n    // console.log(\"1111\");\n  },\n\n  methods: {\n    showImg() {\n      var file = document.getElementById('img_file').files[0];\n      var re = new FileReader();\n      re.readAsDataURL(file);\n\n      re.onload = function (re) {\n        document.getElementById('img_id').src = re.target.result;\n      };\n    },\n\n    doRegister() {\n      if (!this.user.username) {\n        this.$message.error(\"请输入用户名！\");\n        return;\n      } else if (!this.user.peoplename) {\n        this.$message.error(\"请输入姓名！\");\n        return;\n      } else if (!this.user.job) {\n        this.$message.error(\"请输入职位\");\n        return;\n      } else if (this.user.job != null) {\n        if (!this.user.password) {\n          this.$message.error(\"请输入密码！\");\n          return;\n        } else {\n          //this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\n          let url = 'http://localhost:8080/register';\n          axios.post(url, {\n            User_Id: this.user.username,\n            Position: this.user.job,\n            User_Password: this.user.password,\n            User_Name: this.user.peoplename\n          }).then(res => {\n            // console.log(\"输出response.data\", res.data);\n            // console.log(\"输出response.data.status\", res.data.status);\n            if (res.data == 0) {\n              alert(\"注册成功！！\");\n              this.$router.push({\n                path: \"/\"\n              });\n            } else {\n              alert(\"您输入的用户id已存在！\");\n            }\n          });\n        }\n      }\n    },\n\n    handleAvatarSuccess(res, file) {\n      this.imageUrl = URL.createObjectURL(file.raw);\n    },\n\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n\n      return isJPG && isLt2M;\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyCA;AACA;EACAA,aADA;;EAEAC;IACA;MACAC;QACAC,YADA;QAEAC,OAFA;QAGAC,YAHA;QAIAC;MAJA,CADA;MAOAC;IAPA;EASA,CAZA;;EAaAC,WACA;IACA;EACA,CAhBA;;EAiBAC;IACAC;MACA;MACA;MAEAC;;MACAA;QACAC;MACA,CAFA;IAGA,CATA;;IAWAC;MACA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;MACA,CAHA,MAGA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;UACA;UACAC,MACAC,IADA,CACAC,GADA,EACA;YACAC,2BADA;YAEAC,uBAFA;YAGAC,iCAHA;YAIAC;UAJA,CADA,EAOAC,IAPA,CAOAC;YACA;YACA;YACA;cACAC;cACA;gBAAAC;cAAA;YACA,CAHA,MAGA;cACAD;YACA;UACA,CAhBA;QAiBA;MACA;IACA,CA/CA;;IAgDAE;MACA;IACA,CAlDA;;IAmDAC;MACA;MACA;;MAEA;QACA;MACA;;MACA;QACA;MACA;;MACA;IACA;;EA9DA;AAjBA","names":["name","data","user","username","job","password","peoplename","imageUrl","created","methods","showImg","re","document","doRegister","axios","post","url","User_Id","Position","User_Password","User_Name","then","res","alert","path","handleAvatarSuccess","beforeAvatarUpload"],"sourceRoot":"src/components","sources":["Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"login clearfix\">\r\n    <div class=\"login-wrap\">\r\n      <el-row type=\"flex\" justify=\"center\">\r\n        <el-form ref=\"loginForm\" :model=\"user\" status-icon label-width=\"80px\">\r\n          <h3>注册</h3>\r\n          <hr>\r\n          <el-form-item prop=\"username\" label=\"账号\">\r\n            <el-input v-model=\"user.username\" placeholder=\"请输入用户名\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"username\" label=\"昵称\">\r\n            <el-input v-model=\"user.username\" placeholder=\"请输入用昵称\"></el-input>\r\n          </el-form-item>\r\n          \r\n          <el-form-item prop=\"password\" label=\"设置密码\">\r\n            <el-input v-model=\"user.password\" show-password placeholder=\"请输入密码\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"job\" label=\"电话号码\">\r\n            <el-input v-model=\"user.job\" placeholder=\"请输入电话号码\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <div style=\"height: 70px; width: 70px;\">\r\n              <el-upload \r\n   class=\"avatar-uploader\"\r\n  action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n  :show-file-list=\"false\"\r\n  :on-success=\"handleAvatarSuccess\"\r\n  :before-upload=\"beforeAvatarUpload\">\r\n  <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\">\r\n  <i v-else class=\"el-icon-plus avatar-uploader-icon\" style=\"font-size: 10px;\">头像</i>\r\n</el-upload>\r\n            </div>\r\n            <el-button type=\"primary\" icon @click=\"doRegister()\" style=\"margin: 30px 20px;\" >注册账号</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-row>\r\n    </div>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"login\",\r\n  data() {\r\n    return {\r\n      user: {\r\n        username: \"\",\r\n        job: \"\",\r\n        password: \"\",\r\n        peoplename: \"\"\r\n      },\r\n      imageUrl: ''\r\n    };\r\n  },\r\n  created() {\r\n    // console.log($);\r\n    // console.log(\"1111\");\r\n  },\r\n  methods: {\r\n     showImg(){\r\n\tvar file = document.getElementById('img_file').files[0];\r\n\tvar re = new FileReader();\r\n  \r\n\tre.readAsDataURL(file);\r\n\tre.onload = function(re){\r\n\t\tdocument.getElementById('img_id').src=re.target.result;\r\n\t}\r\n},\r\n\r\n    doRegister() {\r\n      if (!this.user.username) {\r\n        this.$message.error(\"请输入用户名！\");\r\n        return;\r\n      } else if (!this.user.peoplename) {\r\n        this.$message.error(\"请输入姓名！\");\r\n        return;\r\n      } else if (!this.user.job) {\r\n        this.$message.error(\"请输入职位\");\r\n        return;\r\n      } else if (this.user.job != null) {\r\n          if (!this.user.password) {\r\n            this.$message.error(\"请输入密码！\");\r\n          return;\r\n        } else {\r\n           //this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\r\n           let url = 'http://localhost:8080/register';\r\n          axios\r\n            .post(url, {\r\n              User_Id: this.user.username,\r\n              Position: this.user.job,\r\n              User_Password: this.user.password,\r\n              User_Name: this.user.peoplename\r\n            })\r\n            .then(res => {\r\n               // console.log(\"输出response.data\", res.data);\r\n               // console.log(\"输出response.data.status\", res.data.status);\r\n              if (res.data==0) {\r\n                alert(\"注册成功！！\");\r\n                this.$router.push({ path: \"/\" });\r\n              } else {\r\n                alert(\"您输入的用户id已存在！\");\r\n              }\r\n            });\r\n        }\r\n      }\r\n    },\r\n    handleAvatarSuccess(res, file) {\r\n        this.imageUrl = URL.createObjectURL(file.raw);\r\n      },\r\n      beforeAvatarUpload(file) {\r\n        const isJPG = file.type === 'image/jpeg';\r\n        const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n        if (!isJPG) {\r\n          this.$message.error('上传头像图片只能是 JPG 格式!');\r\n        }\r\n        if (!isLt2M) {\r\n          this.$message.error('上传头像图片大小不能超过 2MB!');\r\n        }\r\n        return isJPG && isLt2M;\r\n      }\r\n  }\r\n};\r\n\r\n</script>\r\n \r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.login {\r\n  position:absolute;\r\n  top:0px;\r\n  left: 0px;\r\n  width: 100%;\r\n  height: 960px;\r\n  z-index: 9999;\r\n  background: url(\"../assets/login.jpg\") no-repeat;\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n.login-wrap {\r\n  background: white;\r\n  background-size: cover;\r\n  opacity: 0.8;\r\n  width: 500px;\r\n  height: 600px;\r\n  margin: 300px 1280px;\r\n  overflow: hidden;\r\n  padding-top: 10px;\r\n  line-height: 20px;\r\n}\r\n \r\nh3 {\r\n  color: #0babeab8;\r\n  font-size: 24px;\r\n}\r\nhr {\r\n  background-color: #444;\r\n  margin: 20px auto;\r\n}\r\n \r\n.el-button {\r\n  width: 80%;\r\n  margin-left: 5px;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n    border: 2px dashed #070606;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    \r\n  }\r\n  .avatar-uploader .el-upload:hover {\r\n    border-color: #c06510;\r\n  }\r\n  .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #2450b68b;\r\n    width: 100px;\r\n    height: 100px;\r\n    line-height: 100px;\r\n    text-align: center;\r\n  }\r\n  .avatar {\r\n    width: 100px;\r\n    height: 100px;\r\n    display: block;\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module"}