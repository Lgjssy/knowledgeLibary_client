(option);\n    myChart2.setOption(option);\n    myChart3.setOption(option); //建议加上以下这一行代码，不加的效果图如下（当浏览器窗口缩小的时候）。超过了div的界限（红色边框）\n\n    window.addEventListener('resize', function () {\n      myChart1.resize();\n    });\n    window.addEventListener('resize', function () {\n      myChart2.resize();\n    });\n    window.addEventListener('resize', function () {\n      myChart3.resize();\n    });\n  },\n\n  methods: {\n    selectbox(val) {\n      this.values = [];\n      val.array.forEach(item => {\n        const value = item.split(':')[0];\n        this.values.push(value);\n      });\n    },\n\n    ChartManage() {\n      if (this.values.includes(\"chart1\")) {\n        this.one = 1;\n      } else {\n        this.one = 0;\n      }\n    }\n\n  },\n  watch: {},\n\n  created() {}\n\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6HA;AACA;AACA;EACAA;IACA;MACAC,iBADA;MAEAC,UAFA;MAGAC,UAHA;MAIAC,MAJA;MAKAC,MALA;MAMAC,QANA;MAOAC,eAPA;MAQAN,iBARA;MASAO,oBATA;MAUAC;IAVA;EAYA,CAdA;;EAeAC;IACA;IACA;IACA;IACA;IACA;MACAC;QACAC,4BADA;QAEAC;MAFA,CADA;MAKAC;QACAC;MADA,CALA;MAQAC,SACA;QACAC,mBADA;QAEAC,WAFA;QAGAC,aAHA;QAIAnB,OACA;UAAAoB;UAAAH;QAAA,CADA,EAEA;UAAAG;UAAAH;QAAA,CAFA,EAGA;UAAAG;UAAAH;QAAA,CAHA,EAIA;UAAAG;UAAAH;QAAA,CAJA,EAKA;UAAAG;UAAAH;QAAA,CALA,CAJA;QAWAI;UACAC;YACAC,cADA;YAEAC,gBAFA;YAGAC;UAHA;QADA;MAXA,CADA;IARA;IA8BAC;IACAC;IACAC,2BArCA,CAsCA;;IACAC;MAAAH;IAAA;IACAG;MAAAF;IAAA;IACAE;MAAAD;IAAA;EACA,CAzDA;;EA0DAE;IAEAC;MACA;MACAC;QACA;QACA;MACA,CAHA;IAIA,CARA;;IASAC;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA;IACA;;EAfA,CA1DA;EA2EAC,SA3EA;;EA4EAC,WAEA;;AA9EA","names":["data","checkedCharts","values","labels","one","two","three","checkAll","charts","isIndeterminate","mounted","title","text","left","tooltip","trigger","series","name","type","radius","value","emphasis","itemStyle","shadowBlur","shadowOffsetX","shadowColor","myChart1","myChart2","myChart3","window","methods","selectbox","val","ChartManage","watch","created"],"sourceRoot":"src/components","sources":["Chart.vue"],"sourcesContent":["<template>\r\n        <el-main style=\"padding:10px;margin:10px;height:600px\">\r\n        <div style=\"position:absolute;left: 20px;top:138px;background-color: white;width: 299px;height:600px ;z-index:9999;\">\r\n        <h3 style=\"position: relative;left: 40px;color: black;\">请选择报表类型</h3>\r\n          <template>\r\n          \r\n          <el-checkbox-group v-model=\"checkedCharts\" @change=\"selectbox\">\r\n            <el-checkbox label=\"chart1\"  style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">按项目统计bug数</el-checkbox>\r\n            <el-checkbox label=\"chart2\"  style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">按模块统计bug数</el-checkbox>\r\n            <el-checkbox label=\"chart3\" style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">每天新增的bug数</el-checkbox>\r\n            <el-checkbox label=\"chart4\" style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">每日关闭bug数</el-checkbox>\r\n            <el-checkbox label=\"chart5\" style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">每人创建bug数</el-checkbox>\r\n            <el-checkbox label=\"chart6\" style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">每人完成bug数</el-checkbox>\r\n            <el-checkbox label=\"chart7\"  style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">按bug类型统计bug数</el-checkbox>\r\n            <el-checkbox label=\"chart8\" style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">按紧急程度统计bug数</el-checkbox>\r\n            <el-checkbox label=\"chart9\"  style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">按优先级统计bug数</el-checkbox>\r\n            <el-checkbox label=\"chart10\"  style=\"display: block;padding-top: 10px;position: relative;left: 35px;color: lightskyblue;\">'按状态统计bug数'</el-checkbox>\r\n          </el-checkbox-group>\r\n        </template>\r\n        <el-button type=\"primary\" size=\"medium\" @click=\"ChartManage\" style=\"margin-left:70px;margin-top:20px;\">生成报表</el-button>\r\n        </div>\r\n      <div style=\"position:absolute;left:350px;top:138px;width:1150px;height:92px;background-color: white;z-index: 99;\">\r\n      <div style=\"display:flex\">\r\n        <div style=\"position: absolute;left:10px;top:12px\">\r\n          <el-input \r\n            placeholder=\"请输入内容\"\r\n            maxlength=\"10\"\r\n            size=\"medium\"\r\n            v-model=\"search\"\r\n            clearable>\r\n          </el-input> \r\n        </div>\r\n        <el-button type=\"primary\" size=\"medium\" style=\"position: absolute;left: 240px;top:12px \">+筛选</el-button>\r\n        <el-button type=\"primary\" size=\"medium\" icon=\"el-icon-search\"  style=\"position: absolute;left: 315px;top:11.5px\">搜索</el-button>\r\n      </div>\r\n      <div style=\"display:flex\">\r\n        <h3 style=\"position: absolute;left:10px;top:32px;color:lightskyblue\">饼状图</h3>\r\n        <p style=\"position: absolute;left:10px;top:62px;color:lightgray;font-size: 12px;\">注：统计报表的数据来源于列表页面的检索结果，生成统计报表前请先在列表页面进行检索。比如列表页面我们检索的是未关闭Bug，那么报表就是基于之前检索的未关闭Bug的结果集进行统计</p>\r\n      </div>\r\n      </div>\r\n      <div v-show=\"one\" style=\"margin-left:600px;margin-top: 80px;float: left;\"><el-table\r\n      :data=\"tableData\"\r\n      style=\"width: 100%\"\r\n      max-height=\"250\">\r\n      <el-table-column\r\n        prop=\"item\"\r\n        label=\"条目\"\r\n        width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"num\"\r\n        label=\"值\">\r\n      </el-table-column>\r\n    </el-table></div>\r\n    <div id=\"chart_example1\" v-show=\"one\"></div>\r\n    <div v-show=\"two\" style=\"margin-left:600px;margin-top: 80px;float: left;\"><el-table\r\n      :data=\"tableData\"\r\n      style=\"width: 100%\"\r\n      max-height=\"250\">\r\n      <el-table-column\r\n        prop=\"item\"\r\n        label=\"条目\"\r\n        width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"num\"\r\n        label=\"值\">\r\n      </el-table-column>\r\n    </el-table></div>\r\n    <div id=\"chart_example2\" v-show=\"two\"></div>\r\n    <div v-show=\"three\" style=\"margin-left:600px;margin-top: 80px;float: left;\"><el-table\r\n      :data=\"tableData\"\r\n      style=\"width: 100%\"\r\n      max-height=\"250\">\r\n      <el-table-column\r\n        prop=\"item\"\r\n        label=\"条目\"\r\n        width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"num\"\r\n        label=\"值\">\r\n      </el-table-column>\r\n    </el-table></div>\r\n    <div id=\"chart_example3\" v-show=\"three\"></div>\r\n    </el-main>\r\n\r\n</template>\r\n<style>\r\n  .el-main {\r\n    background-color: white;\r\n    color: #D3DCE6;\r\n  }\r\n  h2{\r\n    text-align: center;\r\n    padding: 30px;\r\n    font-size: 18px;\r\n  }\r\n  #chart_example1{\r\n    width: 550px;\r\n    height: 250px;\r\n    background-color: gainsboro;\r\n    margin-top: 90px;\r\n    position: relative;\r\n    left: 10px;\r\n  }\r\n #chart_example2{\r\n    width: 550px;\r\n    height: 250px;\r\n    background-color: gainsboro;\r\n    margin-top: 90px;\r\n    position: relative;\r\n    left: 10px;\r\n  }\r\n  #chart_example3{\r\n    width: 550px;\r\n    height: 250px;\r\n    background-color: gainsboro;\r\n    margin-top: 90px;\r\n    position: relative;\r\n    left: 10px;\r\n  }\r\n</style>\r\n \r\n<script>\r\n  import * as echarts from 'echarts'\r\n  const chartOptions = ['全选'];\r\n  export default {\r\n    data()  {\r\n      return {\r\n        checkedCharts:[],\r\n        values:[],\r\n        labels:[],\r\n        one:0,\r\n        two:0,\r\n        three:0,\r\n        checkAll: false,\r\n        checkedCharts: [],\r\n        charts: chartOptions,\r\n        isIndeterminate: true\r\n      }\r\n    },\r\n    mounted() {\r\n      //let this_ = this;\r\n      let myChart1 = echarts.init(document.getElementById('chart_example1'));\r\n      let myChart2 = echarts.init(document.getElementById('chart_example2'));\r\n      let myChart3 = echarts.init(document.getElementById('chart_example3'));\r\n      let option = {\r\n              title: {\r\n          text: 'Referer of a Website',\r\n          left: 'center'\r\n        },\r\n        tooltip: {\r\n          trigger: 'item'\r\n        },\r\n        series: [\r\n          {\r\n            name: 'Access From',\r\n            type: 'pie',\r\n            radius: '50%',\r\n            data: [\r\n              { value: 1048, name: 'Search Engine' },\r\n              { value: 735, name: 'Direct' },\r\n              { value: 580, name: 'Email' },\r\n              { value: 484, name: 'Union Ads' },\r\n              { value: 300, name: 'Video Ads' }\r\n            ],\r\n            emphasis: {\r\n              itemStyle: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n              }\r\n            }\r\n          }\r\n        ]\r\n      };\r\n      myChart1.setOption(option);\r\n      myChart2.setOption(option);\r\n      myChart3.setOption(option);\r\n      //建议加上以下这一行代码，不加的效果图如下（当浏览器窗口缩小的时候）。超过了div的界限（红色边框）\r\n      window.addEventListener('resize',function() {myChart1.resize()});\r\n      window.addEventListener('resize',function() {myChart2.resize()});\r\n      window.addEventListener('resize',function() {myChart3.resize()});\r\n    },\r\n    methods: {\r\n\r\n      selectbox(val){\r\n        this.values=[];\r\n        val.array.forEach(item => {\r\n          const value = item.split(':')[0];\r\n          this.values.push(value);\r\n        });\r\n      },\r\n      ChartManage(){\r\n        if(this.values.includes(\"chart1\")){\r\n          this.one = 1;\r\n        }else {\r\n          this.one = 0;\r\n        }\r\n        }\r\n    },\r\n    watch: {},\r\n    created() {\r\n \r\n    }\r\n  }\r\n</script>\r\n\r\n"]},"metadata":{},"sourceType":"module"}