{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  name: \"login\",\n\n  data() {\n    return {\n      user: {\n        username: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  created() {// console.log($);\n    // console.log(\"1111\");\n  },\n\n  methods: {\n    doRegister() {\n      if (!this.user.username) {\n        this.$message.error(\"请输入用户名！\");\n        return;\n      } else if (!this.user.email) {\n        this.$message.error(\"请输入邮箱！\");\n        return;\n      } else if (this.user.email != null) {\n        var reg = /^[A-Za-z0-9\\u4e00-\\u9fa5]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$/;\n\n        if (!reg.test(this.user.email)) {\n          this.$message.error(\"请输入有效的邮箱！\");\n        } else if (!this.user.password) {\n          this.$message.error(\"请输入密码！\");\n          return;\n        } else {\n          // this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\n          axios.post(\"/register/\", {\n            name: this.user.username,\n            email: this.user.email,\n            password: this.user.password\n          }).then(res => {\n            // console.log(\"输出response.data\", res.data);\n            // console.log(\"输出response.data.status\", res.data.status);\n            if (res.data.status === 200) {\n              this.$router.push({\n                path: \"/\"\n              });\n            } else {\n              alert(\"您输入的用户名已存在！\");\n            }\n          });\n        }\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;AA0BA;AACA;EACAA,aADA;;EAEAC;IACA;MACAC;QACAC,YADA;QAEAC,SAFA;QAGAC;MAHA;IADA;EAOA,CAVA;;EAWAC,WACA;IACA;EACA,CAdA;;EAeAC;IACAC;MACA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;MACA,CAHA,MAGA;QACA;;QACA;UACA;QACA,CAFA,MAEA;UACA;UACA;QACA,CAHA,MAGA;UACA;UACAC,MACAC,IADA,CACA,YADA,EACA;YACAV,wBADA;YAEAI,sBAFA;YAGAC;UAHA,CADA,EAMAM,IANA,CAMAC;YACA;YACA;YACA;cACA;gBAAAC;cAAA;YACA,CAFA,MAEA;cACAC;YACA;UACA,CAdA;QAeA;MACA;IACA;;EAlCA;AAfA","names":["name","data","user","username","email","password","created","methods","doRegister","axios","post","then","res","path","alert"],"sourceRoot":"src/components","sources":["register.vue"],"sourcesContent":["<template>\r\n  <div class=\"login clearfix\">\r\n    <div class=\"login-wrap\">\r\n      <el-row type=\"flex\" justify=\"center\">\r\n        <el-form ref=\"loginForm\" :model=\"user\" status-icon label-width=\"80px\">\r\n          <h3>注册</h3>\r\n          <hr>\r\n          <el-form-item prop=\"username\" label=\"用户名\">\r\n            <el-input v-model=\"user.username\" placeholder=\"请输入用户名\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"email\" label=\"邮箱\">\r\n            <el-input v-model=\"user.email\" placeholder=\"请输入邮箱\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\" label=\"设置密码\">\r\n            <el-input v-model=\"user.password\" show-password placeholder=\"请输入密码\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" icon @click=\"doRegister()\">注册账号</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-row>\r\n    </div>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"login\",\r\n  data() {\r\n    return {\r\n      user: {\r\n        username: \"\",\r\n        email: \"\",\r\n        password: \"\"\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    // console.log($);\r\n    // console.log(\"1111\");\r\n  },\r\n  methods: {\r\n    doRegister() {\r\n      if (!this.user.username) {\r\n        this.$message.error(\"请输入用户名！\");\r\n        return;\r\n      } else if (!this.user.email) {\r\n        this.$message.error(\"请输入邮箱！\");\r\n        return;\r\n      } else if (this.user.email != null) {\r\n        var reg = /^[A-Za-z0-9\\u4e00-\\u9fa5]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$/;\r\n        if (!reg.test(this.user.email)) {\r\n          this.$message.error(\"请输入有效的邮箱！\");\r\n        } else if (!this.user.password) {\r\n          this.$message.error(\"请输入密码！\");\r\n          return;\r\n        } else {\r\n          // this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\r\n          axios\r\n            .post(\"/register/\", {\r\n              name: this.user.username,\r\n              email: this.user.email,\r\n              password: this.user.password\r\n            })\r\n            .then(res => {\r\n              // console.log(\"输出response.data\", res.data);\r\n              // console.log(\"输出response.data.status\", res.data.status);\r\n              if (res.data.status === 200) {\r\n                this.$router.push({ path: \"/\" });\r\n              } else {\r\n                alert(\"您输入的用户名已存在！\");\r\n              }\r\n            });\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n \r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.login {\r\n  width: 100%;\r\n  height: 740px;\r\n  background: transparent;\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n.login-wrap {\r\n  background: transparent;\r\n  background-size: cover;\r\n  width: 600px;\r\n  height: 500px;\r\n  margin: 215px auto;\r\n  overflow: hidden;\r\n  padding-top: 10px;\r\n  line-height: 20px;\r\n}\r\n \r\nh3 {\r\n  color: #0babeab8;\r\n  font-size: 24px;\r\n}\r\nhr {\r\n  background-color: #444;\r\n  margin: 20px auto;\r\n}\r\n \r\n.el-button {\r\n  width: 80%;\r\n  margin-left: -50px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}