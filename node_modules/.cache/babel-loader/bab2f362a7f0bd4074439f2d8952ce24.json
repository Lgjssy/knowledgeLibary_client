el-option label=\"单元测试阶段\" value=\"shanghai\"></el-option>\r\n      <el-option label=\"功能测试阶段\" value=\"beijing\"></el-option>\r\n    </el-select>\r\n  </el-form-item>\r\n  <el-form-item label=\"用例标题\">\r\n    <el-input v-model=\"form.name\"></el-input>\r\n  </el-form-item>\r\n   <el-form-item label=\"优先级别\">\r\n    <el-select v-model=\"form.region\" placeholder=\"请选择优先级别\">\r\n      <el-option label=\"优先级1\" value=\"shanghai\"></el-option>\r\n      <el-option label=\"优先级2\" value=\"beijing\"></el-option>\r\n    </el-select>\r\n  </el-form-item>\r\n  \r\n  <el-form-item label=\"前置条件\">\r\n    <el-input type=\"textarea\" v-model=\"form.desc\"></el-input>\r\n  </el-form-item>\r\n  \r\n  <el-form-item>\r\n    <el-button type=\"primary\" @click=\"onSubmit\">立即创建</el-button>\r\n    <el-button>取消</el-button>\r\n  </el-form-item>\r\n</el-form>\r\n  <el-button slot=\"reference\" type=\"primary\" style=\"margin-left:1100px;margin-top:10px\">新建用例</el-button>\r\n</el-popover>\r\n      </div>\r\n      <div style=\"display:flex\">\r\n        <div style=\"margin-left:20px;margin-bottom:10px;display: flex;\">\r\n          <el-input\r\n            placeholder=\"请输入用例标题\"\r\n            maxlength=\"10\"\r\n            size=\"small\"\r\n            v-model=\"input\"\r\n            clearable>\r\n          </el-input>\r\n          <el-button type=\"primary\" style=\"margin: 0 10px;height: 32px;width: 70px;\">搜索</el-button>\r\n        </div>\r\n      </div>\r\n      <el-table\r\n        :data=\"tableData\"\r\n        style=\"width: 100%;text-align: center\"\r\n        :default-sort = \"{prop: 'Bug_Title', order: 'descending'}\"\r\n        >\r\n\r\n        <el-table-column\r\n          prop=\"ID\"\r\n          label=\"ID\"\r\n          sortable\r\n          width=\"80\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Priority\"\r\n          label=\"P\"\r\n          sortable\r\n          width=\"80\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Title\"\r\n          label=\"用例标题\"\r\n          sortable\r\n          width=\"200\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Type\"\r\n          label=\"类型\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Creator\"\r\n          label=\"创建人\"\r\n          sortable\r\n          width=\"110\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Do_people\"\r\n          label=\"执行人\"\r\n          sortable\r\n          width=\"110\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Do_time\"\r\n          label=\"执行时间\"\r\n          width=\"130\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Result\"\r\n          label=\"结果\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"status\"\r\n          label=\"状态\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Bug_num\"\r\n          label=\"B\"\r\n          width=\"50\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Res_num\"\r\n          label=\"R\"\r\n          width=\"50\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Step_num\"\r\n          label=\"S\"\r\n          width=\"50\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Operation\"\r\n          label=\"操作\"\r\n          fixed=\"right\"\r\n          width=\"240\">\r\n          <el-button icon=\"el-icon-date\" type=\"text\" style=\"font-size:15px\"></el-button>\r\n          <el-button icon=\"el-icon-video-play\"  style=\"font-size:15px\"  type=\"text\"></el-button>\r\n          <el-button icon=\"el-icon-edit-outline\"  style=\"font-size:15px\" type=\"text\"></el-button>\r\n