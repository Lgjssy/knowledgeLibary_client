{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from 'axios';\nlet id = 1000;\nexport default {\n  methods: {\n    //主页面-bug模块-表格\n    formatter(row, column) {\n      return row.address;\n    },\n\n    //顶层标签页切换\n    handleSelect(key, keyPath) {\n      console.log(key, keyPath);\n    },\n\n    //模块管理\n    methodManage() {\n      this.leftShow1 = !this.leftShow1, this.leftShow2 = !this.leftShow2, this.leftShow3 = !this.leftShow3;\n    },\n\n    //模块选择树\n    append(data) {\n      const newChild = {\n        id: id++,\n        label: 'testtest',\n        children: []\n      };\n\n      if (!data.children) {\n        this.$set(data, 'children', []);\n      }\n\n      data.children.push(newChild);\n    },\n\n    FindAll() {\n      let url1 = 'http://localhost:8081/findBugAllNum';\n      let n = 0;\n      axios.get(url1).then(res => {\n        alert(res.data);\n      });\n      let url2 = 'http://localhost:8081/findBugAll';\n      axios.post(url2).then(res => {\n        for (let i = 0; i < n; i++) {\n          this.tableData.push({\n            Bug_Title: res.data[i][0],\n            Priority: res.data[i][1],\n            Bug_Status: res.data[i][2],\n            Creator: res.data[i][3],\n            creat_time: res.data[i][4],\n            Bug_End_Date: res.data[i][5],\n            Assignee: res.data[i][6],\n            Severity: res.data[i][7],\n            Operating_System: res.data[i][8],\n            Related_Modules: res.data[i][9],\n            Related_Tasks: res.data[i][10],\n            Related_Needs: res.data[i][11],\n            Person_In_Charge: res.data[i][12]\n          });\n        }\n      });\n    },\n\n    //提bug到表单中\n    subform() {\n      this.tableData.push({\n        Bug_Title: this.form1.name,\n        Priority: this.form1.priority,\n        Bug_Status: this.form1.bug_status,\n        Creator: this.form1.creator,\n        creat_time: this.form1.date1,\n        Bug_End_Date: this.form1.date2,\n        Assignee: this.form1.assignee,\n        Severity: this.form1.severity,\n        Operating_System: this.form1.os,\n        Related_Modules: this.form1.modules,\n        Related_Tasks: this.form1.tasks,\n        Related_Needs: this.form1.requires,\n        Person_In_Charge: this.form1.charge\n      });\n    },\n\n    //bug删除方法\n    deletebug(node, data) {\n      this.splice(scope.$index);\n    },\n\n    remove(node, data) {\n      const parent = node.parent;\n      const children = parent.data.children || parent.data;\n      const index = children.findIndex(d => d.id === data.id);\n      children.splice(index, 1);\n    },\n\n    renderContent(h, {\n      node,\n      data,\n      store\n    }) {\n      return (//编辑方法为空\n        h(\"span\", {\n          \"class\": \"custom-tree-node\"\n        }, [h(\"span\", [node.label]), h(\"span\", [h(\"el-button\", {\n          \"attrs\": {\n            \"icon\": \"el-icon-circle-plus-outline\",\n            \"size\": \"mini\",\n            \"type\": \"text\"\n          },\n          \"on\": {\n            \"click\": () => this.append(data)\n          }\n        }), h(\"el-button\", {\n          \"attrs\": {\n            \"icon\": \"el-icon-edit\",\n            \"size\": \"mini\",\n            \"type\": \"text\"\n          }\n        }), h(\"el-button\", {\n          \"attrs\": {\n            \"icon\": \"el-icon-delete\",\n            \"size\": \"mini\",\n            \"type\": \"text\"\n          },\n          \"on\": {\n            \"click\": () => this.remove(node, data)\n          }\n        })])])\n      );\n    },\n\n    //主页面-bug模块-标签页选择\n    handleClick(tab, event) {\n      console.log(tab, event);\n    }\n\n  },\n\n  data() {\n    const data = [{\n      id: 1,\n      label: '用例管理',\n      children: [{\n        id: 4,\n        label: '用例first',\n        children: [{\n          id: 9,\n          label: '三级 1-1-1'\n        }, {\n          id: 10,\n          label: '三级 1-1-2'\n        }]\n      }]\n    }, {\n      id: 2,\n      label: '一级 2',\n      children: [{\n        id: 5,\n        label: '二级 2-1'\n      }, {\n        id: 6,\n        label: '二级 2-2'\n      }]\n    }, {\n      id: 3,\n      label: '一级 3',\n      children: [{\n        id: 7,\n        label: '二级 3-1'\n      }, {\n        id: 8,\n        label: '二级 3-2'\n      }]\n    }];\n    return {\n      labelPosition: 'right',\n      form1: {\n        name: '',\n        priority: '',\n        bug_status: '',\n        creator: '',\n        date1: '',\n        date2: '',\n        assignee: '',\n        severity: '',\n        os: '',\n        modules: '',\n        tasks: '',\n        requires: '',\n        charge: '',\n        type: '',\n        delivery: false,\n        type: [],\n        resource: '',\n        desc: ''\n      },\n      //弹窗默认关闭\n      dialogForm1Visible: false,\n      //主页面-bug模块-表格\n      tableData: [{}],\n      //主页面-bug模块-搜索框\n      input: '',\n      //顶层标签页切换\n      activeIndex2: '1',\n      //左侧模块\n      //树部分\n      data: JSON.parse(JSON.stringify(data)),\n      data: JSON.parse(JSON.stringify(data)),\n      //模块管理部分\n      leftShow1: true,\n      leftShow2: false,\n      leftShow3: false,\n      //项目选择部分\n      options: [{\n        value: '选项1',\n        label: '黄金糕'\n      }, {\n        value: '选项2',\n        label: '双皮奶'\n      }, {\n        value: '选项3',\n        label: '蚵仔煎'\n      }, {\n        value: '选项4',\n        label: '龙须面'\n      }, {\n        value: '选项5',\n        label: '北京烤鸭'\n      }],\n      value: '',\n      props: {\n        label: 'name',\n        children: 'zones'\n      },\n      count: 1\n    };\n  }\n\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4SA;AACA;AACA;EACAA;IACA;IACAC;MACA;IACA,CAJA;;IAKA;IACAC,2BACA;MACAC;IACA,CATA;;IAWA;IACAC;MACA,kCACA,gCADA,EAEA,gCAFA;IAGA,CAhBA;;IAkBA;IACAC;MACA;QAAAC;QAAAC;QAAAC;MAAA;;MACA;QACA;MACA;;MACAC;IACA,CAzBA;;IA0BAC;MACA;MACA;MACAC;QACAC;MACA,CAFA;MAIA;MACAD;QACA;UAEA,oBACA;YACAE,yBADA;YAEAC,wBAFA;YAGAC,0BAHA;YAIAC,uBAJA;YAKAC,0BALA;YAMAC,4BANA;YAOAC,wBAPA;YAQAC,wBARA;YASAC,gCATA;YAUAC,+BAVA;YAWAC,8BAXA;YAYAC,8BAZA;YAaAC;UAbA,CADA;QAgBA;MACA,CApBA;IAuBA,CAzDA;;IA2DA;IACAC;MACA,oBACA;QACAb,0BADA;QAEAC,6BAFA;QAGAC,iCAHA;QAIAC,2BAJA;QAKAC,4BALA;QAMAC,8BANA;QAOAC,6BAPA;QAQAC,6BARA;QASAC,+BATA;QAUAC,mCAVA;QAWAC,+BAXA;QAYAC,kCAZA;QAaAC;MAbA,CADA;IAmBA,CAhFA;;IAmFA;IACAE;MACA;IACA,CAtFA;;IAyFAC;MACA;MACA;MACA;MACApB;IACA,CA9FA;;IAgGAqB;MAAAC;MAAArB;MAAAsB;IAAA;MACA,OACA;QADA;UAAA,SAEA;QAFA,eAGAD,UAHA;UAAA;YAAA,QAKA,6BALA;YAAA,QAKA,MALA;YAAA,QAKA;UALA;UAAA;YAAA,SAKA;UALA;QAAA;UAAA;YAAA,QAMA,cANA;YAAA,QAMA,MANA;YAAA,QAMA;UANA;QAAA;UAAA;YAAA,QAOA,gBAPA;YAAA,QAOA,MAPA;YAAA,QAOA;UAPA;UAAA;YAAA,SAOA;UAPA;QAAA;MAAA;IAUA,CA3GA;;IA4GA;IACAE;MACA7B;IACA;;EA/GA,CADA;;EAkHAM;IAEA;MACAH,KADA;MAEAC,aAFA;MAGAC;QACAF,KADA;QAEAC,gBAFA;QAGAC;UACAF,KADA;UAEAC;QAFA,GAGA;UACAD,MADA;UAEAC;QAFA,CAHA;MAHA;IAHA,GAcA;MACAD,KADA;MAEAC,aAFA;MAGAC;QACAF,KADA;QAEAC;MAFA,GAGA;QACAD,KADA;QAEAC;MAFA,CAHA;IAHA,CAdA,EAwBA;MACAD,KADA;MAEAC,aAFA;MAGAC;QACAF,KADA;QAEAC;MAFA,GAGA;QACAD,KADA;QAEAC;MAFA,CAHA;IAHA,CAxBA;IAoCA;MAGA0B,sBAHA;MAIAC;QACAC,QADA;QAEAC,YAFA;QAGAC,cAHA;QAIAC,WAJA;QAKAC,SALA;QAMAC,SANA;QAOAC,YAPA;QAQAC,YARA;QASAC,MATA;QAUAC,WAVA;QAWAC,SAXA;QAYAC,YAZA;QAaAC,UAbA;QAcAC,QAdA;QAeAC,eAfA;QAgBAD,QAhBA;QAiBAE,YAjBA;QAkBAC;MAlBA,CAJA;MAwBA;MACAC,yBAzBA;MA2BA;MACAC,eA5BA;MAgCA;MACAC,SAjCA;MAkCA;MACAC,iBAnCA;MAoCA;MACA;MACA9C,sCAtCA;MAuCAA,sCAvCA;MAwCA;MACA+C,eAzCA;MA0CAC,gBA1CA;MA2CAC,gBA3CA;MA4CA;MACAC;QACAC,YADA;QAEArD;MAFA,GAGA;QACAqD,YADA;QAEArD;MAFA,CAHA,EAMA;QACAqD,YADA;QAEArD;MAFA,CANA,EASA;QACAqD,YADA;QAEArD;MAFA,CATA,EAYA;QACAqD,YADA;QAEArD;MAFA,CAZA,CA7CA;MA6DAqD,SA7DA;MA8DAC;QACAtD,aADA;QAEAC;MAFA,CA9DA;MAkEAsD;IAlEA;EAoEA;;AA5NA","names":["methods","formatter","handleSelect","console","methodManage","append","id","label","children","data","FindAll","axios","alert","Bug_Title","Priority","Bug_Status","Creator","creat_time","Bug_End_Date","Assignee","Severity","Operating_System","Related_Modules","Related_Tasks","Related_Needs","Person_In_Charge","subform","deletebug","remove","renderContent","node","store","handleClick","labelPosition","form1","name","priority","bug_status","creator","date1","date2","assignee","severity","os","modules","tasks","requires","charge","type","delivery","resource","desc","dialogForm1Visible","tableData","input","activeIndex2","leftShow1","leftShow2","leftShow3","options","value","props","count"],"sourceRoot":"src/components","sources":["Bug.vue"],"sourcesContent":["<template>\r\n\r\n\r\n    <el-main style=\"padding:10px;margin:10px;height:100%\" >\r\n      <div style=\"display:flex\">\r\n        <h3 style=\"margin-left:10px;margin-top:10px;color:black\">我的BUG</h3>\r\n        <el-button type=\"primary\" @click=\"dialogForm1Visible = true\" style=\"margin-left:70%;margin-top:10px\">+提BUG</el-button>\r\n        <!--弹窗部分--> \r\n      <el-dialog title=\"新增Bug\" :visible.sync=\"dialogForm1Visible\" >\r\n    \r\n  <el-form :label-position=\"labelPosition\" :model=\"form1\" >\r\n\r\n    <el-row >\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"Bug标题\"  label-width=\"80px\" >\r\n            <el-input  placeholder=\"请输入bug标题\" v-model=\"form1.name\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\" 优先级\" label-width=\"80px\" style=\"margin:0 80px\">\r\n            <el-select v-model=\"form1.priority\" placeholder=\"请选择优先级\">\r\n              <el-option label=\"1\" value=\"1\"></el-option>\r\n              <el-option label=\"2\" value=\"2\"></el-option>\r\n              <el-option label=\"3\" value=\"3\"></el-option>\r\n              <el-option label=\"4\" value=\"4\"></el-option>\r\n              <el-option label=\"5\" value=\"5\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n    </el-row>\r\n\r\n <el-row >\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"相关模块\"  label-width=\"80px\"   >\r\n            <el-input  placeholder=\"请输入内容\" v-model=\"form1.modules\" autocomplete=\"off\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n      <el-form-item label=\"bug状态\" label-width=\"80px\" style=\"margin:0 80px\">\r\n            <el-select v-model=\"form1.bug_status\" placeholder=\"请选择状态\">\r\n              <el-option label=\"未确认\" value=\"1\"></el-option>\r\n              <el-option label=\"已确认\" value=\"2\"></el-option>\r\n              <el-option label=\"未解决\" value=\"3\"></el-option>\r\n              <el-option label=\"已解决\" value=\"4\"></el-option>\r\n              <el-option label=\"已关闭\" value=\"5\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n      </el-col>\r\n  </el-row>\r\n\r\n  <el-row >\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"相关任务\"  label-width=\"80px\">\r\n            <el-input placeholder=\"请输入相关任务\" v-model=\"form1.tasks\" autocomplete=\"off\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"严重程度\" label-width=\"80px\" style=\"margin:0 80px\">\r\n            <el-select v-model=\"form1.severity\" placeholder=\"请选择严重程度\">\r\n              <el-option label=\"高\" value=\"1\"></el-option>\r\n              <el-option label=\"较高\" value=\"2\"></el-option>\r\n              <el-option label=\"一般\" value=\"3\"></el-option>\r\n              <el-option label=\"较低\" value=\"4\"></el-option>\r\n              <el-option label=\"低\" value=\"5\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n      </el-col>\r\n  </el-row>\r\n\r\n\r\n\r\n  <el-row >\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"相关需求\" label-width=\"80px\">\r\n            <el-input  placeholder=\"请输入相关需求\" v-model=\"form1.requires\" autocomplete=\"off\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"操作系统\" label-width=\"80px\" style=\"margin:0 80px\">\r\n            <el-select v-model=\"form1.os\" placeholder=\"请选择使用的操作系统\">\r\n              <el-option label=\"Windows\" value=\"1\"></el-option>\r\n              <el-option label=\"Linux\" value=\"2\"></el-option>\r\n              <el-option label=\"ios\" value=\"3\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n      </el-col>\r\n  </el-row>\r\n\r\n\r\n\r\n <el-row >\r\n      <el-col :span=\"8\">    \r\n          <el-form-item label=\"创建人\"  label-width=\"80px\"  >\r\n            <el-input placeholder=\"请输入创建人的姓名\" v-model=\"form1.creator\" autocomplete=\"off\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n\r\n      <el-col :span=\"8\">  \r\n          <el-form-item label=\"受指派人\" label-width=\"80px\"   >\r\n            <el-input placeholder=\"请输入受指派人的姓名\" v-model=\"form1.assignee\" autocomplete=\"off\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n\r\n      <el-col :span=\"8\">  \r\n          <el-form-item label=\"负责人\"  label-width=\"80px\"   >\r\n            <el-input placeholder=\"请输入负责人姓名\" v-model=\"form1.charge\" autocomplete=\"off\"></el-input>              \r\n          </el-form-item>\r\n      </el-col>\r\n  </el-row>\r\n\r\n <el-row >\r\n      <el-col :span=\"12\">\r\n          <el-form-item label=\"创建日期\" label-width=\"80px\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form1.date1\" style=\"width: 100%;\"></el-date-picker>\r\n            </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n            <el-form-item label=\"截止日期\" label-width=\"80px\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form1.date2\" style=\"width: 100%;\"></el-date-picker>\r\n            </el-form-item>  \r\n      </el-col>         \r\n</el-row>       \r\n</el-form>\r\n          <div slot=\"footer\" class=\"dialog-footer\" >\r\n            <el-button @click=\"dialogForm1Visible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"subform();dialogForm1Visible = false\">确 定</el-button>\r\n          </div>\r\n        </el-dialog>\r\n      </div>\r\n\r\n\r\n      <div style=\"display:flex\">\r\n        <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\">\r\n          <el-tab-pane label=\"指派给我\" name=\"first\">指派给我</el-tab-pane>\r\n          <el-tab-pane label=\"我创建的\" name=\"second\">我创建的</el-tab-pane>\r\n          <el-tab-pane label=\"我关闭的\" name=\"third\">我关闭的</el-tab-pane>\r\n          <el-tab-pane label=\"我完成的\" name=\"fourth\">我完成的</el-tab-pane>\r\n          <el-tab-pane label=\"全部\" name=\"fifth\" >全部</el-tab-pane>\r\n        </el-tabs>\r\n        <div style=\"margin-left:20px;margin-bottom:10px\">\r\n          <el-input\r\n            placeholder=\"请输入内容\"\r\n            maxlength=\"10\"\r\n            size=\"small\"\r\n            v-model=\"input\"\r\n            clearable>\r\n          </el-input>\r\n        </div><el-button type=\"primary\" style=\"margin: 0 10px;height: 32px;width: 70px;\" @click=\"FindAll()\" >搜索</el-button>\r\n      </div>\r\n      <el-table\r\n        :data=\"tableData\"\r\n        style=\"width: 100%;text-align: center\"\r\n        :default-sort = \"{prop: 'Bug_Title', order: 'descending'}\"\r\n        >\r\n\r\n        <el-table-column\r\n          prop=\"Bug_Title\"\r\n          label=\"Bug标题\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Priority\"\r\n          label=\"优先级\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Bug_Status\"\r\n          label=\"状态\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Creator\"\r\n          label=\"创建者\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"creat_time\"\r\n          label=\"创建日期\"\r\n          sortable\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Bug_End_Date\"\r\n          label=\"结束日期\"\r\n          sortable\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Assignee\"\r\n          label=\"指派人\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table