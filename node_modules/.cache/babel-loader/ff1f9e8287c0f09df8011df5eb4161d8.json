width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"num\"\r\n        label=\"值\">\r\n      </el-table-column>\r\n    </el-table></div>\r\n    <div id=\"chart_example3\" v-show=\"three\"></div>\r\n    </el-main>\r\n\r\n</template>\r\n<style>\r\n  .el-main {\r\n    background-color: white;\r\n    color: #D3DCE6;\r\n  }\r\n  h2{\r\n    text-align: center;\r\n    padding: 30px;\r\n    font-size: 18px;\r\n  }\r\n  #chart_example1{\r\n    width: 550px;\r\n    height: 250px;\r\n    background-color: gainsboro;\r\n    margin-top: 90px;\r\n    position: relative;\r\n    left: 10px;\r\n  }\r\n #chart_example2{\r\n    width: 550px;\r\n    height: 250px;\r\n    background-color: gainsboro;\r\n    margin-top: 90px;\r\n    position: relative;\r\n    left: 10px;\r\n  }\r\n  #chart_example3{\r\n    width: 550px;\r\n    height: 250px;\r\n    background-color: gainsboro;\r\n    margin-top: 90px;\r\n    position: relative;\r\n    left: 10px;\r\n  }\r\n</style>\r\n \r\n<script>\r\n  import * as echarts from 'echarts'\r\n\r\n  export default {\r\n    data()  {\r\n      return {\r\n        one:0,\r\n        two:0,\r\n        three:0,\r\n        data: [{\r\n          id: 1,\r\n          label: '全部',\r\n          children: [{\r\n            id: 2,\r\n            label: '按项目统计Bug数',\r\n            },{\r\n            id: 3,\r\n            label: '按模块统计Bug数',\r\n            },\r\n            {\r\n            id: 4,\r\n            label: '每天新增的Bug数',\r\n            },\r\n            {\r\n            id: 5,\r\n            label: '每日关闭Bug数',\r\n            },\r\n            {\r\n            id: 6,\r\n            label: '每人创建Bug数',\r\n            },\r\n            {\r\n            id: 7,\r\n            label: '每人完成Bug数',\r\n            },\r\n            {\r\n            id: 8,\r\n            label: '每人关闭Bug数',\r\n            },\r\n            {\r\n            id: 9,\r\n            label: '按Bug类型统计Bug数',\r\n            },\r\n            {\r\n            id: 10,\r\n            label: '按优先级统计Bug数',\r\n            },\r\n            {\r\n            id: 11,\r\n            label: '按状态统计Bug数',\r\n            },\r\n            ]\r\n        }, ]\r\n      }\r\n    },\r\n    mounted() {\r\n      //let this_ = this;\r\n      let myChart1 = echarts.init(document.getElementById('chart_example1'));\r\n      let myChart2 = echarts.init(document.getElementById('chart_example2'));\r\n      let myChart3 = echarts.init(document.getElementById('chart_example3'));\r\n      let option = {\r\n              title: {\r\n          text: 'Referer of a Website',\r\n          left: 'center'\r\n        },\r\n        tooltip: {\r\n          trigger: 'item'\r\n        },\r\n        series: [\r\n          {\r\n            name: 'Access From',\r\n            type: 'pie',\r\n            radius: '50%',\r\n            data: [\r\n              { value: 1048, name: 'Search Engine' },\r\n              { value: 735, name: 'Direct' },\r\n              { value: 580, name: 'Email' },\r\n              { value: 484, name: 'Union Ads' },\r\n              { value: 300, name: 'Video Ads' }\r\n            ],\r\n            emphasis: {\r\n              itemStyle: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n              }\r\n            }\r\n          }\r\n        ]\r\n      };\r\n      myChart1.setOption(option);\r\n      myChart2.setOption(option);\r\n      myChart3.setOption(option);\r\n      //建议加上以下这一行代码，不加的效果图如下（当浏览器窗口缩小的时候）。超过了div的界限（红色边框）\r\n      window.addEventListener('resize',function() {myChart1.resize()});\r\n      window.addEventListener('resize',function() {myChart2.resize()});\r\n      window.addEventListener('resize',function() {myChart3.resize()});\r\n    },\r\n    methods: {\r\n      Treenode(){\r\n        let res = this.$refs.chartTree.getCheckedNodes()\r\n        let arr = []\r\n        res.forEach(item => {\r\n          arr.push(item.id)\r\n        });\r\n      },\r\n      ChartManage(){\r\n        if(arr.includes(2) == 2)\r\n        this.one = 1;\r\n        else if(this.data(id) == 3)\r\n        this.two = 1;\r\n        else if(this.data(id) == 4)\r\n        this.three = 1\r\n      }\r\n    },\r\n    watch: {},\r\n    created() {\r\n \r\n    }\r\n  }\r\n</script>\r\n\r\n"]},"metadata":{},"sourceType":"module"}