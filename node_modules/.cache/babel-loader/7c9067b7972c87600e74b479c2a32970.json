,CAnHA;MAwHAb;MACA;QAAAc;QAAAD;MAAA,CADA,EAEA;QAAAC;QAAAD;MAAA,CAFA,EAGA;QAAAC;QAAAD;MAAA,CAHA,CAxHA;MA6HAK,8BA7HA;MA8HA3B;QACAE,WADA;QAEAN,YAFA;QAGAQ,aAHA;QAIAC,WAJA;QAKAC,UALA;QAMAd,6BANA;QAOAe,QAPA;QAQAE,WARA;QASAD;MATA;IA9HA;EA2IA;;AAtLA","names":["methods","Acivate","Save","Start","jumpToUseCase","updateInput","submitTest","associate","onSubmit","Test_Plan_Id","Test_Plan_Name","Owning_module","Human_resource_allocation","Start_Date","End_Date","State","Priority","Description","Test_risk","data","submitPlanForm","id","project","people","StartDate","EndDate","status","name","description","handler","risk","form","region","date1","date2","delivery","type","resource","desc","PlanData","word_description_situationDialog","word_basicInformation_situationDialog","label","value","word_historyRecord_situationDialog","situationDialogVisible","projects","submitPlanDialogVisible"],"sourceRoot":"src/components","sources":["Plan.vue"],"sourcesContent":["<template>\r\n  \r\n    <el-container\r\n      style=\"\r\n        background-color: #e5f2ff;\r\n        height: 100%;\r\n        margin: 0px;\r\n        max-width: 1050px;\r\n      \"\r\n    >\r\n      <el-main style=\"padding: 10px; margin: 10px; height: 100%\">\r\n        <div style=\"display: flex\">\r\n          <h3 style=\"margin-left: 10px; margin-top: 10px; color: black\">\r\n            测试计划\r\n          </h3>\r\n          <el-button\r\n            type=\"primary\"\r\n            style=\"margin-left: 70%; margin-top: 10px\"\r\n            @click=\"submitPlanDialogVisible = true\"\r\n            >+提交测试</el-button\r\n          >\r\n        </div>\r\n<!--提交测试弹框---------------------------------------------------------------------------------------------------------->\r\n            <el-dialog title=\"提交计划\" :visible.sync=\"submitPlanDialogVisible\" width=\"80%\">\r\n      <el-form   v-model=\"submitPlanForm\" label-width=\"100px\">\r\n\r\n           <el-form-item label=\"id\">\r\n            <el-input  v-model=\"submitPlanForm.id\"></el-input>\r\n          </el-form-item>\r\n\r\n  <el-form-item label=\"风险\">\r\n    <el-select v-model=\"submitPlanForm.risk\" placeholder=\"请选择风险程度\">\r\n      <el-option label=\"1\" value=\"1\"></el-option>\r\n      <el-option label=\"2\" value=\"2\"></el-option>\r\n            <el-option label=\"3\" value=\"3\"></el-option>\r\n                  <el-option label=\"4\" value=\"4\"></el-option>\r\n                        <el-option label=\"5\" value=\"5\"></el-option>\r\n    </el-select>\r\n  </el-form-item>\r\n\r\n          <el-form-item label=\"所属模块\">\r\n              <el-select v-model=\"submitPlanForm.project\" placeholder=\"请选择\">\r\n\r\n              <el-option \r\n                v-for=\"project in projects\"\r\n                :key=\"project.value\"\r\n                :value=\"project.value\"\r\n                :label=\"project.label\"\r\n              ></el-option>\r\n              </el-select>\r\n          </el-form-item> \r\n          <el-form-item label=\"项目负责人\">\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"6\">\r\n                <el-input  v-model=\"submitPlanForm.people\"></el-input>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <label>优先级</label>\r\n                <el-select label=\"优先级\" v-model=\"submitPlanForm.Priority\" placeholder=\"请选择\">\r\n                  <el-option \r\n                    v-for=\"Pri in Priority\"\r\n                    :key=\"Pri.value\"\r\n                    :value=\"Pri.value\"\r\n                    :label=\"Pri.label\"\r\n                  ></el-option>\r\n                </el-select>  \r\n              </el-col>\r\n            </el-row>\r\n          </el-form-item> \r\n          <el-form-item label=\"开始日期\">\r\n              <el-col :span=\"8\">\r\n              <el-date-picker type=\"date\" placeholder=\"选择开始日期\" v-model=\"submitPlanForm.StartDate\" width=\"60%\"></el-date-picker>\r\n              </el-col>\r\n              <el-col class=\"line\" :span=\"2\">-</el-col>\r\n              <el-col :span=\"12\">\r\n              <label>结束日期</label>\r\n              <el-date-picker type=\"date\" placeholder=\"选择结束日期\" v-model=\"submitPlanForm.EndDate\" width=\"60%\"></el-date-picker>\r\n              </el-col>\r\n          </el-form-item>\r\n          <el-form-item :gutter=\"20\">\r\n            <el-row>\r\n              <el-col :span=\"12\">\r\n                <label>状态</label>\r\n                <el-select  v-model=\"submitPlanForm.status\" placeholder=\"请选择\">\r\n                  <el-option \r\n                    v-for=\"sta in status\"\r\n                    :key=\"sta.value\"\r\n                    :value=\"sta.value\"\r\n                    :label=\"sta.label\"\r\n                  ></el-option>\r\n                </el-select>  \r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <label>人力资源分配</label>\r\n                <el-select  v-model=\"submitPlanForm.Human_resource_allocation\" placeholder=\"请选择\">\r\n                  <el-option \r\n                    v-for=\"hra in Human_resource_allocation\"\r\n                    :key=\"hra.value\"\r\n                    :value=\"hra.value\"\r\n                    :label=\"hra.label\"\r\n                  ></el-option>\r\n                </el-select> \r\n              </el-col>\r\n            </el-row>          \r\n          </el-form-item>\r\n          <el-form-item label=\"名称\">\r\n            <el-input  v-model=\"submitPlanForm.name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"描述\">\r\n            <el-input\r\n              :rows=\"8\"\r\n              type=\"textarea\"\r\n              placeholder=\"请输入内容\"\r\n              v-model=\"submitPlanForm.description\"\r\n              maxlength=\"100\"\r\n              show-word-limit\r\n            >\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"抄送给\">\r\n              <el-select v-model=\"submitPlanForm.handler\" placeholder=\"请选择\">\r\n              <el-option \r\n                v-for=\"han in handler\"\r\n                :key=\"han.value\"\r\n                :value=\"han.value\"\r\n                :label=\"han.label\"\r\n              ></el-option>\r\n              </el-select>\r\n          </el-form-item>\r\n          <el-form-item>\r\n              <el-button type=\"primary\" @click=\"onSubmit();submitPlanDialogVisible = false\">保存</el-button>\r\n              <el-button @click=\"submitPlanDialogVisible = false\">取消</el-button>\r\n          </el-form-item> \r\n      </el-form> \r\n    </el-dialog>\r\n<!---------------------------------------------------------------------------------------------------------->\r\n\r\n        <div style=\"display: flex\">\r\n          <div style=\"margin-left: 20px; margin-bottom: 10px\">\r\n            <el-input\r\n              placeholder=\"请输入内容\"\r\n              maxlength=\"10\"\r\n              size=\"small\"\r\n              v-model=\"input\"\r\n              clearable\r\n            >\r\n            </el-input>\r\n          </div>\r\n        </div>\r\n        <el-table\r\n          :data=\"PlanData\"\r\n          style=\"width: 100%; text-align: center\"\r\n          :default-sort=\"{ prop: 'Test_Plan_Id', order: 'descending' }\"\r\n        >\r\n          <el-table-column prop=\"Test_Plan_Id\" label=\"ID\" sortable width=\"100\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"Test_Plan_Name\" label=\"名称\" sortablewidth=\"100\">\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"Owning_module\"\r\n            label=\"所属模块\"\r\n            sortable\r\n            width=\"100\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"Human_resource_allocation\"\r\n            label=\"人力资源分配\"\r\n            sortable\r\n            width=\"180\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"Start_Date\"\r\n            label=\"开始日期\"\r\n            sortable\r\n            width=\"180\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"End_Date\"\r\n            label=\"结束日期\"\r\n            sortable\r\n            width=\"180\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column prop=\"State\" label=\"状态\" width=\"100\" sortable>\r\n          </el-table-column>\r\n          <el-table-column prop=\"Priority\" label=\"优先级\" width=\"100\" sortable>\r\n          </el-table-column>\r\n          <el-table-column prop=\"Description\" label=\"描述\" width=\"100\" sortable>\r\n          </el-table-column>\r\n          <el-table-column prop=\"Test_risk\" label=\"测试风险\" width=\"100\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"Operation\" label=\"操作\" fixed=\"right\" width=\"240\">\r\n<!---------------------------------------------------------------------------------------------------------->\r\n          <router-link to=\"/Plan/select\"><!-- 查看包含的用例 -->\r\n            <el-button\r\n              icon=\"el-icon-folder-opened\"\r\n              type=\"text\"\r\n              style=\"font-size: 15px\"\r\n              @click=\"jumpToUseCase\"\r\n            ></el-button>\r\n          </router-link>\r\n<!---------------------------------------------------------------------------------------------------------->\r\n            <el-button  \r\n              icon=\"el-icon-notebook-2\"\r\n              style=\"font-size: 15px\"\r\n              type=\"text\"\r\n              @click=\"situationDialogVisible = true\"\r\n            ></el-button> <!--查看概况-->\r\n\r\n            <el-button\r\n              icon=\"el-icon-edit-outline\"\r\n              style=\"font-size: 15px\"\r\n              type=\"text\"\r\n              @click=\"submitPlanDialogVisible = true\"\r\n            ></el-button><!--修改计划-->\r\n            <router-link to=\"/Plan/select\"><!