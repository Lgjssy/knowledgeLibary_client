r\n\r\n  </a-card>\r\n\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nconst data = [];\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      data,\r\n      pagination: {\r\n        onChange: page => {\r\n          console.log(page);\r\n        },\r\n        pageSize: 3,\r\n      },\r\n     \r\n\r\n    }\r\n  },\r\n  created: function () {\r\n    // `this` 指向 vm 实例\r\n    let url='http://localhost:8082/addMyUser'\r\n    axios.post(url,{}).then(res=>{\r\n      console.log(res.data)\r\n      for(let i=0;i<res.data.length;i++){\r\n        data.push({\r\n          title:res.data[i].user_name,\r\n          about:res.data[i].about,\r\n          avatar:'logo.png' \r\n\r\n        })\r\n      }\r\n    })\r\n  }\r\n}\r\n\r\n\r\n</script>\r\n  \r\n<style>\r\n.userCard {\r\n  background-color: white;\r\n  color: cornflowerblue;\r\n  font-size: large;\r\n\r\n  width: 100%;\r\n}\r\n\r\n.a-icon {\r\n  text-align: center;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}