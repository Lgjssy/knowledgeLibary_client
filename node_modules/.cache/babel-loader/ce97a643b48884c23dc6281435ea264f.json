localhost:8082/Details';\r\n    axios.post(url,data1).then(res=>{\r\n     if(res.data==1){\r\n      \r\n       Event.$emit('Component2',{username:this.user.username,isVisual:false})\r\n       this.$message.success('管理员登录成功');\r\n      this.$router.push({name:\"Home\",params: {id:'dsadsa',isVisual:true}})\r\n     }\r\n     else if(res.data==2){\r\n      Event.$emit('Component2',{username:this.user.username,isVisual:true})\r\n      this.$message.success('管理员登录成功');\r\n      this.$router.push({name:\"Home\",params: {id:'dsadsa',isVisual:true}})\r\n     }\r\n    \r\n    \r\n    })\r\n             \r\n          \r\n      \r\n    }\r\n  }\r\n};// \r\n</script>\r\n\r\n\r\n<style scoped>\r\n.login {\r\n  position:absolute;\r\n  top: 0px;\r\n  left: 0px;\r\n  z-index: 100;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: url(\"../assets/login.jpg\") no-repeat;\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n.login-wrap {\r\n  background-color: aliceblue;\r\n  background-size: cover;\r\n  opacity: 0.8;\r\n  width: 25%;\r\n  height: 50%;\r\n  margin:22% 70%;\r\n  overflow: hidden;\r\n  padding-top: 10px;\r\n  line-height: 40px;\r\n}\r\n#password {\r\n  margin-bottom: 5px;\r\n}\r\nh3 {\r\n  color: #0babeab8;\r\n  font-size: 36px;\r\n}\r\nhr {\r\n  background-color: rgb(184, 27, 27);\r\n  margin: 20px auto;\r\n}\r\na {\r\n  text-decoration: none;\r\n  color: #aaa;\r\n  font-size: 15px;\r\n}\r\na:hover {\r\n  color: coral;\r\n}\r\n.el-button {\r\n  width: 80%;\r\n  margin-left: -50px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}