=\"tableData\"\r\n        style=\"width: 100%;text-align: center\"\r\n        :default-sort = \"{prop: 'Bug_Title', order: 'descending'}\">\r\n        \r\n        <el-table-column\r\n          prop=\"Bug_Title\"\r\n          label=\"Bug标题\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Priority\"\r\n          label=\"优先级\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Bug_Status\"\r\n          label=\"状态\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Creator\"\r\n          label=\"创建者\"\r\n          sortable\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"creat_time\"\r\n          label=\"创建日期\"\r\n          sortable\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Bug_End_Date\"\r\n          label=\"结束日期\"\r\n          sortable\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Assignee\"\r\n          label=\"指派人\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Severity\"\r\n          label=\"严重程度\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Operating_System\"\r\n          label=\"操作系统\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Related_Modules\"\r\n          label=\"相关模块\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Related_Tasks\"\r\n          label=\"相关任务\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Related_Needs\"\r\n          label=\"相关需求\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Person_In_Charge\"\r\n          label=\"负责人\"\r\n          width=\"100\"\r\n          sortable>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Operation\"\r\n          label=\"操作\"\r\n          fixed:right\r\n          width=\"240\">\r\n          <el-button icon=\"el-icon-video-play\" type=\"text\" style=\"font-size:15px\"></el-button>\r\n          <el-button icon=\"el-icon-circle-close\"  style=\"font-size:15px\"  type=\"text\"></el-button>\r\n          <el-button icon=\"el-icon-s-opportunity\"  style=\"font-size:15px\" type=\"text\"></el-button>\r\n          <el-button icon=\"el-icon-s-custom\"  style=\"font-size:15px\" type=\"text\"></el-button>\r\n          <el-button icon=\"el-icon-circle-check\"  style=\"font-size:15px\" type=\"text\"></el-button>\r\n          <el-button icon=\"el-icon-edit\"  style=\"font-size:15px\" type=\"text\"></el-button>\r\n          <el-button icon=\"el-icon-delete\"  style=\"font-size:15px\" type=\"text\"></el-button>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-main>\r\n  </el-container>\r\n    </div>\r\n</template>\r\n\r\n<style>\r\n .custom-tree-node {\r\n    flex: 1;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    font-size: 14px;\r\n    padding-right: 8px;\r\n  }\r\n.test{\r\nborder:1px solid  #eaff00;\r\n}\r\n.kqtlt{\r\n      text-align: center;\r\n    line-height: 60px;\r\n}\r\n\r\n.el-header, .el-footer {\r\n    background-color: white;\r\n    color: #D3DCE6;\r\n\r\n  }\r\n  \r\n  .el-aside {\r\n    background-color: white;\r\n    color: #D3DCE6;\r\n\r\n  }\r\n  \r\n  .el-main {\r\n    background-color: white;\r\n    color: #D3DCE6;\r\n  }\r\n  \r\n  body > .el-container {\r\n    margin-bottom: 40px;\r\n  }\r\n  \r\n  .el-container:nth-child(5) .el-aside,\r\n  .el-con